#include <stdio.h>

int main()
{
	int a = 1;
	int b = 1;
	int c = 1;
	int d = 1;
	int i = 0;

	while (a <= 9)
	{
		while (b <= 18)
		{
			while (c <= 37)
			{
				while (d <= 37)
				{
					if (10 * a + 5 * b + 2 * c + d == 100 && a + b + c + d == 40)
					{
						i++;
						printf("%d ten yuan bills ,%d five yuan bills ,%d two yuan bills ,%d one yuan bills \n", a, b, c, d);
						d = 1;
						break;
					}
					else {
						d++;
					}
				}d = 1;
				c++;
			}c = 1;
			b++;
		}b = 1;
		a++;
	}
	printf("how many kinds of combinations: %d\n", i);
}
